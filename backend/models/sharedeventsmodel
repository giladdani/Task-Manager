const mongoose  = require("mongoose");
const Schema = mongoose.Schema;

const SharedEventSchema = new Schema({
    title: String,
    id: String,
    sharedId: String,
    projectTitle: String,
    projectId: String,
    projectSharedId: String,
    start: Date,
    end: Date,
    backgroundColor: String,
    unexportedEvent: Boolean,
    email: String,
    shareWithUser: String,
})

const SharedEvent = mongoose.model('SharedEvent', SharedEventSchema);
module.exports = SharedEvent;